<html>	<head><title>
<%= title %>
</title>
<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<script type="text/javascript">
google.charts.load('current', {packages: ['corechart']});
</script></head><body>
<a href='/'>Home</a>

<%= dataArray2 %>
<div id="container" style="width: 550px; height: 400px; margin: 0 auto"></div>
<div id="barcontainer" style="width: 550px; height: 400px; margin: 0 auto"></div>
<div id="colcontainer" style="width: 550px; height: 400px; margin: 0 auto"></div>
<div id="combocontainer" style="width: 550px; height: 400px; margin: 0 auto"></div>
<script language="JavaScript">
function drawChart() {
 // Define the chart to be drawn.
var data = google.visualization.arrayToDataTable(<%= dataArray %>);


var data2 = google.visualization.arrayToDataTable(<%= dataArray2 %>);

var options = {
legend: { position: 'top', maxLines: 5 },
title:'Selected Cost Centers 2009 - 2014',
 isStacked:true
 };
var options3 = {
legend: { position: 'top', maxLines: 5 },
title:'Selected Cost Centers 2009 - 2014'
 };
var options4 = {
legend: { position: 'top', maxLines: 5 },
title:'Selected Cost Centers 2009 - 2014',
isStacked:true,
 vAxis: {title: 'Millions'},
hAxis: {title: 'Years'},
seriesType: 'bars',
 series: {5: {type: 'line'}}
 };
// Instantiate and draw the chart.
var chart3 = new google.visualization.BarChart(document.getElementById('container'));
var chart = new google.visualization.BarChart(document.getElementById('barcontainer'));
var chart2 = new google.visualization.ColumnChart(document.getElementById('colcontainer'));
var chart4 = new google.visualization.BarChart(document.getElementById('combocontainer'));
 chart.draw(data, options);
 chart2.draw(data, options);
 chart3.draw(data, options3);
 chart4.draw(data2, options4);
}
google.charts.setOnLoadCallback(drawChart);
 </script> </body> </html>

