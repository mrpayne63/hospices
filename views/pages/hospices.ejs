    </header>

    <main>
        <div class="row">
            <div class="col-sm-8">
            
                <div class="jumbotron">


                    <h2>Hospice List</h2>
<ul>
    <% entries.forEach(function(row) { %>
        <li><a href=/hospice/<%= row.entityID%>><%= row.name %></a></li>
    <% }); %>
</ul>


                </div>
                
            </div>
            <div class="col-sm-4">
                
                <div class="well">
                    <h3>Sidebar</h3>
                </div>

            </div>
        </div>
    </main>

    <footer>
        <% include ../partials/footer %>
    </footer>
    
</body>
</html>